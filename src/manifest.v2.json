{
  "short_name": "Nautilus",
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' 'wasm-eval'; object-src 'self';",
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "Nautilus Wallet"
  },
  "background": {
    "page": "js/background.js",
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["js/injector.js"],
      "run_at": "document_start",
      "all_frames": true
    }
  ]
}
